<% provide(:page_title, 'ユーザー 一覧') %>
<h1 class="users_top">ユーザー　一覧ページ</h1>
<div>
  <%= form_with url: users_path, method: :get, local: true do |f| %>
    <%= f.label :search, "ユーザー検索" %>
    <%= text_field_tag :search, params[:search], placeholder: "全体ユーザーを検索" %>
    <%= submit_tag 'ユーザー検索', :name => nil %>
  <% end %>
</div>
<%= will_paginate previous_label: '&lt 前へ', next_label: '次へ &gt' %>
<ul class="users">
  <%= render @users %>
</ul>
<%= will_paginate previous_label: '&lt 前へ', next_label: '次へ &gt' %>
